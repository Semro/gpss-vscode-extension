{
    "name": "gpss",
    "displayName": "gpss",
    "description": "Support for IBM gpss system",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.33.0"
    },
    "publisher": "ibanez&holyblackcat",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "gpss",
            "aliases": ["gpss", "gps"],
            "extensions": [".gpss", ".gps"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "gpss",
            "scopeName": "./",
            "path": "./syntaxes/gpss.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "gpss",
                "path": "./snippets/basicsnippets.json"
            }
        ]
    }
}